<!--components/playlist/playlist.wxml-->
<!-- 精选歌单 -->

<view class='playlist-container' bind:tap='goToMusiclist'>
    <image src='{{playlist.coverImgUrl}}' class="playlist-img"></image>
    <text class="playlist-playcount">{{_count}}</text>
    <text class='playlist-name'>{{playlist.name}}</text>
</view>